local Toggle1 = Section1:CreateToggle("Auto Mob", Settings.AutoMob, function(State)
Settings.AutoMob = State

game:GetService("RunService").Stepped:connect(
    function()
        if Settings.AutoMob then
            pcall(
                function()
                    for i, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                        if v:IsA("BasePart") then
                            v.CanCollide = false
                        end
                    end
                end
            )
        end
    end
)

spawn(
    function()
        while wait() and Settings.AutoMob do
            pcall(
                function()
                    if not game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Model") then
                        game:GetService("VirtualInputManager"):SendKeyEvent(true, 49, false, game)
                    end

                    if not game:GetService("Players").DownSideSpin.PlayerGui:FindFirstChild("SpawnSelection") then
                        if getNearestMob().Humanoid.Health == 0 then
                            repeat
                                wait(5)
                            until game.Players.LocalPlayer.Character
                        end
                        Tween(
                            game.Players.LocalPlayer.Character.HumanoidRootPart,
                            getNearestMob().HumanoidRootPart.Position,
                            Settings.Speed,
                            Settings.Time,
                            true,
                            0
                        )
                        if
                            (getNearestMob():FindFirstChildWhichIsA("Humanoid").Health /
                                getNearestMob():FindFirstChildWhichIsA("Humanoid").MaxHealth *
                                100) <=
                                95 and Settings.InstantKill
                         then
                            getNearestMob().Humanoid.Health = 0
                        end
                    end
                end
            )
        end
    end
)



spawn(
    function()
        while wait(0.5) and Settings.AutoMob do
            pcall(function()
            game:GetService("VirtualInputManager"):SendMouseButtonEvent(0, 0, 0, true, game, 1)
            end)
        end
    end
)

end)

